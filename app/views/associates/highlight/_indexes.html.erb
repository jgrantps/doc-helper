<h1 class="text-xl" id="iddd">associates/highlight/indexes</h1>

  <main class="p-3 inline-flex" id="company_reference_point">
    <% @associate.accounts.each do |account| %>
    
      <div class="flex-shrink-0 ml-3 w-80 mt-2 p-3 bg-gray-100 rounded">
        <h3 clas="text-sm font-medium text-gray-900"><%= account.name.titleize %></h3>
        <ul class="mt-2">
          <li class="mt-3">                  
            <%= render partial: 'accounts/highlight/associate_profiles', locals: {:sorted_packages => @associate.accounts.specific(current_user)} %>   
          </li>            
        </ul>
      </div>
    <% end %>  
  </main>
