<h1 class="text-xs" id="iddd">users/dashboard/indexes</h1>
<main class="p-3 inline-flex">
  <% current_user.companies.each do |company| %>
    <div class="flex-shrink-0 ml-3 w-80 mt-2 p-3 bg-gray-100 rounded">
      <h3 clas="text-sm font-medium text-gray-900"><%= company.name.titleize %></h3>
      <ul class="mt-2">
      <%# as;df%>
        <li class="mt-3">          
          <%= render partial: 'users/dashboard/list', locals: {:ary => company.accounts} %>   
        </li>            
      </ul>
    </div>
  <% end %>  
</main>