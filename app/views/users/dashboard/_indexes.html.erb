<h1 class="text-xs" id="iddd">users/dashboard/indexes</h1>
<main class="p-3 inline-flex">
  <% current_user.accounts.each do |account| %>
    <div class="flex-shrink-0 ml-3 w-80 mt-2 p-3 bg-gray-100 rounded">
      <h3 clas="text-sm font-medium text-gray-900"><%= account.name.titleize %></h3>
      <ul class="mt-2">
        <li class="mt-3">                  
          <%= render partial: 'users/dashboard/package', :collection => current_packages.send(package_status), locals: {:indexes => "indexes", :account => "account", :status => "status",:name => "name", :company => "company", :users => "users"} %>   
        </li>            
      </ul>
    </div>
  <% end %>  
</main>